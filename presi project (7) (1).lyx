\begin_layout Plain Layout

set.seed(1)
\end_layout

\begin_layout Plain Layout

v = 6 ; b = 5 
\end_layout

\begin_layout Plain Layout

mu = 0 ; beta = seq(-(b-1)/2,(b-1)/2,length=5); tau = rep(0,v)
\end_layout

\begin_layout Plain Layout

sim = 10^5
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

F_sim = NULL; F_sim_1 = NULL; F_sim_2 = NULL 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for(i in 1:sim)
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

  data_sim = mu + outer(beta,tau,"+") + matrix(rnorm(b*v),nrow = b)
\end_layout

\begin_layout Plain Layout

  data_sim[1,1] <- 0
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  B.1_dash <- sum(data_sim[1,])          # B.1_dash is vector of B1'
\end_layout

\begin_layout Plain Layout

  T.1_dash <- sum(data_sim[,1])          # T.1_dash is vector of T1'
\end_layout

\begin_layout Plain Layout

  G_dash <- sum(data_sim)                # G_dash is vector of G'
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  x_hat <- (b*B.1_dash+v*T.1_dash-G_dash)/((b-1)*(v-1))
\end_layout

\begin_layout Plain Layout

  x_curl <- B.1_dash/(v-1)
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

                   #### approximate test ####
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  data_sim[1,1] <- x_hat
\end_layout

\begin_layout Plain Layout

  B <- rowSums(data_sim)
\end_layout

\begin_layout Plain Layout

  T <- colSums(data_sim)
\end_layout

\begin_layout Plain Layout

  G <- sum(data_sim)  
\end_layout

\begin_layout Plain Layout

  SSBl = sum(B^2)/v - (G^2/(b*v))
\end_layout

\begin_layout Plain Layout

  SSTr = sum(T^2)/b - (G^2/(b*v)) ; MSTr = SSTr/(v-1)
\end_layout

\begin_layout Plain Layout

  TSS = sum(data_sim^2) - (G^2/(b*v))
\end_layout

\begin_layout Plain Layout

  SSE = TSS - SSBl - SSTr ; MSE = SSE/(b*v-b-v)
\end_layout

\begin_layout Plain Layout

  F_sim[i] <- MSTr/MSE
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  sse_hat=SSE
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

            #### More accurate testing procedure-1  ####
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  SSBl = (B.1_dash^2)/(v-1) + (sum(B[-1]^2)/v) - (G_dash^2/(b*v - 1)) 
\end_layout

\begin_layout Plain Layout

  TSS = sum(data_sim^2)-data_sim[1,1]^2 - (G_dash^2/(b*v - 1))
\end_layout

\begin_layout Plain Layout

  SSTr = TSS - SSBl - sse_hat ;  MSTr = SSTr/(v-1)
\end_layout

\begin_layout Plain Layout

  F_sim_1[i] <- MSTr/(sse_hat/(b*v-b-v))
\end_layout

\begin_layout Plain Layout

 
\end_layout

\begin_layout Plain Layout

            #### More accurate testing procedure-2  ####
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

  data_sim[1,1] <- x_curl
\end_layout

\begin_layout Plain Layout

  B <- rowSums(data_sim)
\end_layout

\begin_layout Plain Layout

  T <- colSums(data_sim)
\end_layout

\begin_layout Plain Layout

  G <- sum(data_sim)
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  SSBl = sum(B^2)/v - (G^2/(b*v))
\end_layout

\begin_layout Plain Layout

  SSTr = sum(T^2)/b - (G^2/(b*v))
\end_layout

\begin_layout Plain Layout

  TSS = sum(data_sim^2) - (G^2/(b*v))
\end_layout

\begin_layout Plain Layout

  SSE0 = TSS - SSBl
\end_layout

\begin_layout Plain Layout

  F_sim_2[i] <- ((SSE0 - SSE)/(v-1))/(sse_hat/(b*v-b-v))
\end_layout

\begin_layout Plain Layout

}
\end_layout

